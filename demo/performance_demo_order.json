          {
            "id": "Sleeper_3",
            "name": "Sleeper Timer",
            "description": "An activity to Sleep for a random number of Seconds",
            "activity": {
              "ref": "github.com/slcorp/RTView-Flogo/activity/Sleeper",
              "input": {
                "Min": 500,
                "Rnd": 500
